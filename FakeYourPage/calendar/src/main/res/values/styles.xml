<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="CalendarTextAppearance" parent="android:TextAppearance"></style>

    <style name="CalendarTextAppearance.Week_Header_Label">
        <item name="android:textSize">10dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="CalendarTextAppearance.Week_Day_Num">
        <item name="android:textSize">22dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="CalendarTextAppearance.Week_Day_Lunar">
        <item name="android:translationY">-12px</item>
        <item name="android:textSize">10dp</item>
        <item name="android:textColor">#ffffff</item>
    </style>

</resources>
